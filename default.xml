<?xml version="1.0" encoding="UTF-8"?>
<!-- Used in combination with google repo to check out multiple repositories -->
<manifest>
  
  <default sync-j="4" revision="sumo"/>

  <remote fetch="https://git.yoctoproject.org/git" name="yocto"/>
  <remote fetch="https://git.yoctoproject.org/git" name="meta-intel"/>
  <remote fetch="https://github.com/openembedded" name="oe"/>
  <remote fetch="https://github.com/meta-qt5" name="qt5"/>

  <project remote="yocto" revision="thud" name="poky" path="sources/poky"/>
  <project remote="meta-intel" revision="thud" name="meta-intel" path="sources/meta-intel"/>
  <project remote="oe" revision="thud" name="meta-openembedded" path="sources/meta-openembedded"/>
  <project remote="qt5" revision="jansa/thud" name="meta-qt5" path="sources/meta-qt5"/>
  
</manifest>
